{
  "rewrites": [
    { "source": "/to/:match/:url*", "destination": "https://:match/:url*" },
    { "source": "/to/:match/:url*/", "destination": "https://:match/:url*/" },
    { "source": "/too/:match/:url*", "destination": "http://:match/:url*" },
    { "source": "/too/:match/:url*/", "destination": "http://:match/:url*/" }
  ]
}
