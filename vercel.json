{
  "rewrites": [
    {
      "source": "/gh/:path*",
      "destination": "https://github.com/:path*"
    },
    {
      "source": "/raw/:path*",
      "destination": "https://raw.githubusercontent.com/:path*"
    },
    {
      "source": "/:url(?!(gh/|raw/).*)",
      "destination": "https://:url*"
    }
  ]
}
